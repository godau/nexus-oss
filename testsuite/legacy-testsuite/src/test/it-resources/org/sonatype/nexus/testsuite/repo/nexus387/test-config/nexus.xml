<?xml version="1.0" encoding="UTF-8"?>
<nexusConfiguration>
  <version>2.7.0</version>
  <globalConnectionSettings>
    <connectionTimeout>10000</connectionTimeout>
    <retrievalRetryCount>3</retrievalRetryCount>
    <queryString></queryString>
  </globalConnectionSettings>
  <restApi>
    <uiTimeout>60000</uiTimeout>
  </restApi>
  <httpProxy>
    <port>${nexus-proxy-port}</port>
    <proxyPolicy>strict</proxyPolicy>
  </httpProxy>
  <routing>
    <resolveLinks>true</resolveLinks>
  </routing>
  <repositories>
    <repository>
      <id>repo1</id>
      <name>repo1</name>
      <providerRole>org.sonatype.nexus.proxy.repository.Repository</providerRole>
      <providerHint>maven2</providerHint>
      <localStatus>IN_SERVICE</localStatus>
      <notFoundCacheTTL>1440</notFoundCacheTTL>
      <userManaged>true</userManaged>
      <exposed>true</exposed>
      <browseable>true</browseable>
      <writePolicy>ALLOW_WRITE</writePolicy>
      <indexable>true</indexable>
      <searchable>true</searchable>
      <externalConfiguration>
        <proxyMode>ALLOW</proxyMode>
        <artifactMaxAge>1440</artifactMaxAge>
        <itemMaxAge>1440</itemMaxAge>
        <cleanseRepositoryMetadata>false</cleanseRepositoryMetadata>
        <downloadRemoteIndex>false</downloadRemoteIndex>
        <checksumPolicy>WARN</checksumPolicy>
        <repositoryPolicy>release</repositoryPolicy>
      </externalConfiguration>
    </repository>
    <repository>
      <id>repo2</id>
      <name>repo2</name>
      <providerRole>org.sonatype.nexus.proxy.repository.Repository</providerRole>
      <providerHint>maven2</providerHint>
      <localStatus>IN_SERVICE</localStatus>
      <notFoundCacheTTL>1440</notFoundCacheTTL>
      <userManaged>true</userManaged>
      <exposed>true</exposed>
      <browseable>true</browseable>
      <writePolicy>ALLOW_WRITE</writePolicy>
      <indexable>true</indexable>
      <searchable>true</searchable>
      <externalConfiguration>
        <proxyMode>ALLOW</proxyMode>
        <artifactMaxAge>1440</artifactMaxAge>
        <itemMaxAge>1440</itemMaxAge>
        <cleanseRepositoryMetadata>false</cleanseRepositoryMetadata>
        <downloadRemoteIndex>false</downloadRemoteIndex>
        <checksumPolicy>WARN</checksumPolicy>
        <repositoryPolicy>release</repositoryPolicy>
      </externalConfiguration>
    </repository>
    <repository>
      <id>repo3</id>
      <name>repo3</name>
      <providerRole>org.sonatype.nexus.proxy.repository.Repository</providerRole>
      <providerHint>maven2</providerHint>
      <localStatus>IN_SERVICE</localStatus>
      <notFoundCacheTTL>1440</notFoundCacheTTL>
      <userManaged>true</userManaged>
      <exposed>true</exposed>
      <browseable>true</browseable>
      <writePolicy>ALLOW_WRITE</writePolicy>
      <indexable>true</indexable>
      <searchable>true</searchable>
      <externalConfiguration>
        <proxyMode>ALLOW</proxyMode>
        <artifactMaxAge>1440</artifactMaxAge>
        <itemMaxAge>1440</itemMaxAge>
        <cleanseRepositoryMetadata>false</cleanseRepositoryMetadata>
        <downloadRemoteIndex>false</downloadRemoteIndex>
        <checksumPolicy>WARN</checksumPolicy>
        <repositoryPolicy>release</repositoryPolicy>
      </externalConfiguration>
    </repository>
    <repository>
      <id>exclusive-group</id>
      <name>exclusive-group</name>
      <providerRole>org.sonatype.nexus.proxy.repository.GroupRepository</providerRole>
      <providerHint>maven2</providerHint>
      <localStatus>IN_SERVICE</localStatus>
      <notFoundCacheTTL>15</notFoundCacheTTL>
      <userManaged>true</userManaged>
      <exposed>true</exposed>
      <browseable>true</browseable>
      <writePolicy>READ_ONLY</writePolicy>
      <indexable>true</indexable>
      <externalConfiguration>
        <mergeMetadata>true</mergeMetadata>
        <memberRepositories>
          <memberRepository>repo1</memberRepository>
          <memberRepository>repo2</memberRepository>
          <memberRepository>repo3</memberRepository>
        </memberRepositories>
      </externalConfiguration>
    </repository>
    <repository>
      <id>exclusive-single</id>
      <name>exclusive-single</name>
      <providerRole>org.sonatype.nexus.proxy.repository.GroupRepository</providerRole>
      <providerHint>maven2</providerHint>
      <localStatus>IN_SERVICE</localStatus>
      <notFoundCacheTTL>15</notFoundCacheTTL>
      <userManaged>true</userManaged>
      <exposed>true</exposed>
      <browseable>true</browseable>
      <writePolicy>READ_ONLY</writePolicy>
      <indexable>true</indexable>
      <externalConfiguration>
        <mergeMetadata>true</mergeMetadata>
        <memberRepositories>
          <memberRepository>repo1</memberRepository>
        </memberRepositories>
      </externalConfiguration>
    </repository>
    <repository>
      <id>inclusive-single</id>
      <name>inclusive-single</name>
      <providerRole>org.sonatype.nexus.proxy.repository.GroupRepository</providerRole>
      <providerHint>maven2</providerHint>
      <localStatus>IN_SERVICE</localStatus>
      <notFoundCacheTTL>15</notFoundCacheTTL>
      <userManaged>true</userManaged>
      <exposed>true</exposed>
      <browseable>true</browseable>
      <writePolicy>READ_ONLY</writePolicy>
      <indexable>true</indexable>
      <externalConfiguration>
        <mergeMetadata>true</mergeMetadata>
        <memberRepositories>
          <memberRepository>repo1</memberRepository>
        </memberRepositories>
      </externalConfiguration>
    </repository>
    <repository>
      <id>other-group</id>
      <name>other-group</name>
      <providerRole>org.sonatype.nexus.proxy.repository.GroupRepository</providerRole>
      <providerHint>maven2</providerHint>
      <localStatus>IN_SERVICE</localStatus>
      <notFoundCacheTTL>15</notFoundCacheTTL>
      <userManaged>true</userManaged>
      <exposed>true</exposed>
      <browseable>true</browseable>
      <writePolicy>READ_ONLY</writePolicy>
      <indexable>true</indexable>
      <externalConfiguration>
        <mergeMetadata>true</mergeMetadata>
        <memberRepositories>
          <memberRepository>repo1</memberRepository>
          <memberRepository>repo2</memberRepository>
          <memberRepository>repo3</memberRepository>
        </memberRepositories>
      </externalConfiguration>
    </repository>
    <repository>
      <id>inclusive-group</id>
      <name>inclusive-group</name>
      <providerRole>org.sonatype.nexus.proxy.repository.GroupRepository</providerRole>
      <providerHint>maven2</providerHint>
      <localStatus>IN_SERVICE</localStatus>
      <notFoundCacheTTL>15</notFoundCacheTTL>
      <userManaged>true</userManaged>
      <exposed>true</exposed>
      <browseable>true</browseable>
      <writePolicy>READ_ONLY</writePolicy>
      <indexable>true</indexable>
      <externalConfiguration>
        <mergeMetadata>true</mergeMetadata>
        <memberRepositories>
          <memberRepository>repo2</memberRepository>
          <memberRepository>repo1</memberRepository>
          <memberRepository>repo3</memberRepository>
        </memberRepositories>
      </externalConfiguration>
    </repository>
    <repository>
      <id>blocking-group</id>
      <name>blocking-group</name>
      <providerRole>org.sonatype.nexus.proxy.repository.GroupRepository</providerRole>
      <providerHint>maven2</providerHint>
      <localStatus>IN_SERVICE</localStatus>
      <notFoundCacheTTL>15</notFoundCacheTTL>
      <userManaged>true</userManaged>
      <exposed>true</exposed>
      <browseable>true</browseable>
      <writePolicy>READ_ONLY</writePolicy>
      <indexable>true</indexable>
      <externalConfiguration>
        <mergeMetadata>true</mergeMetadata>
        <memberRepositories>
          <memberRepository>repo1</memberRepository>
          <memberRepository>repo2</memberRepository>
          <memberRepository>repo3</memberRepository>
        </memberRepositories>
      </externalConfiguration>
    </repository>
  </repositories>
  <repositoryGrouping>
    <pathMappings>
      <pathMapping>
        <id>exclusive-group</id>
        <groupId>exclusive-group</groupId>
        <routeType>exclusive</routeType>
        <routePatterns>
          <routePattern>.*exclusive.*</routePattern>
        </routePatterns>
        <repositories>
          <repository>repo1</repository>
        </repositories>
      </pathMapping>
      <pathMapping>
        <id>exclusive-single</id>
        <groupId>exclusive-single</groupId>
        <routeType>exclusive</routeType>
        <routePatterns>
          <routePattern>.*exclusive.*</routePattern>
        </routePatterns>
        <repositories>
          <repository>repo1</repository>
        </repositories>
      </pathMapping>
      <pathMapping>
        <id>inclusive-group</id>
        <groupId>inclusive-group</groupId>
        <routeType>inclusive</routeType>
        <routePatterns>
          <routePattern>.*inclusive.*</routePattern>
        </routePatterns>
        <repositories>
          <repository>repo2</repository>
        </repositories>
      </pathMapping>
      <pathMapping>
        <id>inclusive-single</id>
        <groupId>inclusive-single</groupId>
        <routeType>inclusive</routeType>
        <routePatterns>
          <routePattern>.*inclusive.*</routePattern>
        </routePatterns>
        <repositories>
          <repository>repo1</repository>
        </repositories>
      </pathMapping>
      <pathMapping>
        <id>blocking</id>
        <groupId>blocking-group</groupId>
        <routeType>blocking</routeType>
        <routePatterns>
          <routePattern>.*blocking.*</routePattern>
        </routePatterns>
      </pathMapping>
    </pathMappings>
  </repositoryGrouping>
  <smtpConfiguration>
    <hostname>smtp-host</hostname>
    <port>1234</port>
    <username>smtp-username</username>
    <password>smtp-password</password>
    <systemEmailAddress>system@nexus.org</systemEmailAddress>
  </smtpConfiguration>
  <notification />
</nexusConfiguration>
